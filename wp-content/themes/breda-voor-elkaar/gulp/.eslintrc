{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "node": true
    },
    "parserOptions": {
        "ecmaVersion": 7
    },
    "rules": {
        "eol-last": 1,
        "no-else-return": 0,
        "max-len": [1, 150, 2],
        "quotes": [2, "single", { "avoidEscape": true }]
    }
}